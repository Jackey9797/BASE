{
    "logname":"incremental-build_graph",
    "device": "cuda:0",
    
    "/* model related args*/": "//",    
    "x_len": 12,
    "y_len":12, 
    "dropout":0.0, 
    "gcn": {
        "in_channel": 12,
        "out_channel": 12,
        "hidden_channel": 64
        },
    "tcn": {
        "in_channel": 1,
        "out_channel": 1,
        "kernel_size": 3,
        "dilation": 1
        },


    "/*train related args*/": "//",
    "train": true,
    "begin_year": 2011,
    "end_year": 2017,
    "epoch": 100,
    "batch_size": 128,
    "lr": 0.01,
    "loss": "mse",

    
    "/*strategy related args*/": "//",
    "strategy":"incremental",
    "increase": false, 
    "detect": false,
    "replay":false,
    "ewc": false,
    "ewc_strategy": "ewc", 
    "ewc_lambda": 1.0,
    "subgraph_train": false,


    "/*build graph related args*/": "//",
    "build_graph": true,
    "build_subgraph_size": 100, 
    "node_emb_dim": 64, 
    "tanhalpha":3 ,

    "/*instr*/": "python main.py --conf incremental-build_graph > BG3.txt"
}